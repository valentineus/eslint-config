{
  "eol-last": [
    "error",
    "always"
  ],
  "indent-legacy": [
    "off"
  ],
  "lines-around-directive": [
    "off"
  ],
  "newline-after-var": [
    "off"
  ],
  "newline-before-return": [
    "off"
  ],
  "no-catch-shadow": [
    "off"
  ],
  "no-native-reassign": [
    "off"
  ],
  "no-negated-in-lhs": [
    "off"
  ],
  "no-tabs": [
    "error",
    {
      "allowIndentationTabs": true
    }
  ]
}
