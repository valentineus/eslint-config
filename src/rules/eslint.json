{
  "eol-last": [
    "error",
    "always"
  ],
  "indent": [
    "error",
    "tab",
    {
      "ArrayExpression": 1,
      "CallExpression": {
        "arguments": 1
      },
      "FunctionDeclaration": {
        "body": 1,
        "parameters": 1
      },
      "FunctionExpression": {
        "body": 1,
        "parameters": 1
      },
      "ImportDeclaration": 1,
      "MemberExpression": 1,
      "ObjectExpression": 1,
      "SwitchCase": 1,
      "VariableDeclarator": "first",
      "flatTernaryExpressions": true,
      "ignoreComments": false,
      "ignoredNodes": [],
      "outerIIFEBody": 1
    }
  ],
  "indent-legacy": [
    "off"
  ],
  "lines-around-directive": [
    "off"
  ],
  "newline-after-var": [
    "off"
  ],
  "newline-before-return": [
    "off"
  ],
  "no-catch-shadow": [
    "off"
  ],
  "no-native-reassign": [
    "off"
  ],
  "no-negated-in-lhs": [
    "off"
  ],
  "no-spaced-func": [
    "off"
  ],
  "no-tabs": [
    "error",
    {
      "allowIndentationTabs": true
    }
  ],
  "prefer-reflect": [
    "off"
  ],
  "quote-props": [
    "error",
    "consistent-as-needed",
    {
      "keywords": true,
      "numbers": true,
      "unnecessary": true
    }
  ],
  "require-jsdoc": [
    "off"
  ],
  "sort-keys": [
    "warn",
    "asc",
    {
      "caseSensitive": true,
      "minKeys": 2,
      "natural": true
    }
  ],
  "valid-jsdoc": [
    "off"
  ]
}
