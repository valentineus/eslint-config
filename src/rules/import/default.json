{
  "import/exports-last": [
    "error"
  ],
  "import/first": [
    "error"
  ],
  "import/newline-after-import": [
    "error",
    {
      "count": 1
    }
  ],
  "import/no-webpack-loader-syntax": [
    "error"
  ],
  "import/order": [
    "error",
    {
      "alphabetize": {
        "caseInsensitive": false,
        "order": "asc"
      },
      "groups": [
        [
          "builtin",
          "external"
        ],
        [
          "internal",
          "parent",
          "sibling"
        ],
        [
          "index"
        ],
        "unknown"
      ],
      "newlines-between": "always"
    }
  ]
}
